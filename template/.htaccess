RewriteEngine On
RewriteCond %{HTTP_HOST} ^example.com [NC]
RewriteRule ^(.*)$ http://www.consolerepairleeds.co.uk/$1 [L,R=301,NC]
RewriteRule ^index\.html$ / [R=301,L]
RewriteRule ^(.*)/index\.html$ /$1/ [R=301,L]

Options -Indexes

#old version to the new version redirect authority

Redirect 301 http://www.consolerepairleeds.co.uk/xbox.html http://www.consolerepairleeds.co.uk/repairs/xbox/xbox360/index.html
Redirect 301 http://www.consolerepairleeds.co.uk/playstation.html http://www.consolerepairleeds.co.uk/repairs/playstation/ps3/index.html
Redirect 301 http://consolerepairleeds.co.uk/nintendo.html http://consolerepairleeds.co.uk/repairs/nintendo/wii/index.html
Redirect 301 http://www.consolerepairleeds.co.uk/contact.html http://www.consolerepairleeds.co.uk/contact-us.html

#redirect end